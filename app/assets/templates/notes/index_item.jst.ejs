<a href ="#" class="edit-note list-group-item">
  <dl class= >
    <dt>Title</dt>
    <dd>

        <%= note.escape("title") %>

    </dd>
    <dt>Content</dt>
    <dd><%= note.escape("body")%></dd>
    <dd>
      Created: <%= moment(note.get("created_at")).fromNow() %>
    </dd>
    <dd>
      Modified: <%= moment(note.get("updated_at")).fromNow() %>
    </dd>
  </dl>
</a>
