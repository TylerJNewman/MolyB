<ul id="notes-index">
  <% notes.each(function (note) { %>
    <li>
      <dl>
        <dt>Title</dt>
        <dd><%= note.escape("title") %></dd>
        <dt>Content</dt>
        <dd><%= note.escape("body")%></dd>
      </dl>
    </li>
  <% }) %>
</ul>


