<p id="notice"><%= notice %></p>

<h1>Listing Api Notes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_notes.each do |api_note| %>
      <tr>
        <td><%= link_to 'Show', api_note %></td>
        <td><%= link_to 'Edit', edit_api_note_path(api_note) %></td>
        <td><%= link_to 'Destroy', api_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_api_note_path %>
