<p id="notice"><%= notice %></p>

<h1>Listing Api Notebooks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_notebooks.each do |api_notebook| %>
      <tr>
        <td><%= link_to 'Show', api_notebook %></td>
        <td><%= link_to 'Edit', edit_api_notebook_path(api_notebook) %></td>
        <td><%= link_to 'Destroy', api_notebook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notebook', new_api_notebook_path %>
